{
  "files.exclude": {
    ".netlify": true,
    "node_modules": true,
    "**/processed_images": true,
    "**/_gen": true,
    "public": true
  },
  "search.exclude": {
    "static/*.css": true,
  },
  "files.associations": {
    ".html": "handlebars",
    ".css": "tailwindcss"
  },
  "[html][handlebars]": {
    "editor.defaultFormatter": "monosans.djlint",
    "editor.formatOnSave": true
  }
}
